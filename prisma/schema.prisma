  id                  Int          @id @default(autoincrement())
  use_value           Boolean
  is_chooser          Boolean
  show_name           Boolean
  label               String
  aria_label          String
  tooltip             String
  error               String
  value               Json[]
  created_at          DateTime     @default(now()) @db.Timestamp(6)
  updated_at          DateTime?    @db.Timestamp(6)
